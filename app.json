{
  "expo": {
    "name": "PuppyTime",
    "slug": "puppytime",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "puppytime",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.puppytime.app"
    },
    "android": {
      "package": "com.puppytime.app",
      "adaptiveIcon": {
        "backgroundColor": "#FFFBF5",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FFFBF5",
          "dark": {
            "backgroundColor": "#1F2937"
          }
        }
      ],
      "expo-web-browser",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow PuppyTime to access your photos to transform your dog's images.",
          "cameraPermission": "Allow PuppyTime to use your camera to take photos of your dog."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow PuppyTime to save transformed puppy images to your photo library.",
          "savePhotosPermission": "Allow PuppyTime to save images to your photo library.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "experiments": {
      "reactCompiler": true
    }
  }
}
